@using IBS2.Models
@{
    ViewBag.Title = "Statusi banaka";
}

<h2>Lista statusa banaka</h2>

<table class="table table-condensed table-hover">
    <thead>
        <tr>
            <td>Naziv</td>
            <td>Sedište</td>


        </tr>

    </thead>
    @foreach (Banka banka in Admin.ListaBanaka())
    {

        <tr>

            <td>@banka.Naziv</td>
            <td>@banka.Sediste</td>
            <td>@banka.StatusBanke.Status</td>


        </tr>
    }
</table>